pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo 'Собираем проект...'
            }
        }
        stage('Test') {
            steps {
                echo 'Запускаем тесты...'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Деплойим приложение!'
            }
        }
    }
}
